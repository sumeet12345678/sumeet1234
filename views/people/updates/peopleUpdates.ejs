	<script>



	$('#PEOPLE_UPDTS_NOTIFICATION_BTN').on('click', function() {

			var peopleNotifications = $('.people-notification-container');
			$('#PEOPLE_UPDATES_OPTIONS_NOTIFICATION_CNTNR').append(peopleNotifications);
			$('#PEOPLE_UPDATES_OPTIONS_NOTIFICATION_CNTNR').show();
			$('#PEOPLE_UPDATES_OPTIONS_FEED_CNTNR').hide();
			$('#PEOPLE_UPDATES_OPTIONS_NEWS_CNTNR').hide();
			$('.people-notification-container').show();
			peopleNotification();
	});

	
	$('#PEOPLE_UPDTS_FEED_BTN').on('click', function() {

		var peopleUpdates = $('.people-update-container');
			$('#PEOPLE_UPDATES_OPTIONS_FEED_CNTNR').append(peopleUpdates);
			$('#PEOPLE_UPDATES_OPTIONS_FEED_CNTNR').show();
			$('#PEOPLE_UPDATES_OPTIONS_NEWS_CNTNR').hide();
			$('#PEOPLE_UPDATES_OPTIONS_NOTIFICATION_CNTNR').hide();
			$('.people-update-container').show();
			peopleFeedLoad();
	});


	$('#PEOPLE_UPDTS_NEWS_BTN').on('click', function() {
			var peopleNews = $('.people-news-container');
			$('#PEOPLE_UPDATES_OPTIONS_NEWS_CNTNR').append(peopleNews);
			$('#PEOPLE_UPDATES_OPTIONS_NEWS_CNTNR').show();
			$('#PEOPLE_UPDATES_OPTIONS_FEED_CNTNR').hide();
			$('#PEOPLE_UPDATES_OPTIONS_NOTIFICATION_CNTNR').hide();
			$('.people-news-container').show();
			peopleNewsPosts();
	});








    //   Ajax call for PeopleUpdate page

    	var ajaxCallNotification = false;

		function peopleNotification() {
			if(!ajaxCallNotification) {
				showHideProgress();
				$.get('<%=urls.PEOPLE_URL%>/peopleNotifications', function(notification) {
				$('.people-notification-container').append(notification);
				ajaxCallNotification = true;
				showHideProgress();
			});
			}
			
		}


    	var ajaxCallPeopleLoad = false;

		function peopleFeedLoad() {
			if(!ajaxCallPeopleLoad) {
				showHideProgress();
				$.get('<%=urls.PEOPLE_URL%>/peopleFeed', function(data) {
				$('.people-update-container').append(data);
				ajaxCallPeopleLoad = true;
				showHideProgress();
			});
			}
			
		}


		var ajaxCallNewsPosts = false;

		function peopleNewsPosts() {
			if(!ajaxCallNewsPosts) {
				showHideProgress();
				$.get('<%=urls.PEOPLE_URL%>/people_News_Posts', function(news) {
				$('.people-news-container').append(news);
				ajaxCallNewsPosts = true;
				showHideProgress();
			});
			}
			
		}


</script>