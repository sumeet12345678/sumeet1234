                        $('.close-button').on('click', function(){
                        	//alert('llll');
                        	var parent = $(this).parent(); 

                          	if(!parent.hasClass('item-on-popup')) {
                          		 
                          		parent.addClass('item-on-popup');
	                          	parent.appendTo('.attributes');

	                           	//$('.attributes').hide();
	                           	
	                           	$('.attach-attributes').show();
	                           	 
	                        }

                        });

               
                          $(document).mouseup(function (e) 
							 {
							     var 	multiList= $(".multi-list");
							     if (!$('.multi-list').is(e.target) && !multiList.is(e.target) && multiList.has(e.target).length == 0) 
							     {
							         	multiList.hide();
							     }
							 });


                      




                       });

		 				// Removing It From Content Container 
                       function delSelected(closeBtn)
                       {  
                       		var dataAttribValue = closeBtn.parent().attr("data-item-attr");
                          	var index = multiDropDown.indexOf(dataAttribValue);
                          	multiDropDown.splice(index,1);
                       		$(closeBtn).parent().remove();

					   }
                          
	</script>
 