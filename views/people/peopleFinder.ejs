	<script type="text/javascript">
	var multiDropDown = [];
		 $(function()
                       {
						  // Calling Login Form
						  
						  //var i = myArray.indexOf("data-item-attr");
                          $(".multi-selector-contentbox").on('click',function()
                          {
                            var multiList = $(".multi-list")
                           	multiList.show();
                            
                             
                             
                          });
                           // Calling List
                          $(".list-item").on('click',function()
                          {
                          	var dataAttribValue = $(this).attr("data-item-attr");
                          	var index = multiDropDown.indexOf(dataAttribValue);
                          	if(index < 0)
                          	{
                          		multiDropDown.push(dataAttribValue);
                          		var listItem = $(this).text();
	                            var listItemElem = $('<div class="list-item1" contenteditable="false" data-item-attr="' + dataAttribValue +'">' + listItem + '</div>&nbsp; &nbsp;');
	                            	$('<img style="cursor: pointer;" class="itemclose" src="fileclose.png" width="20" height="20" onclick="delSelected($(this))">').appendTo(listItemElem);
	                            listItemElem.prependTo(".multi-selector-contentbox");	
                          	}


                          });


                        $('.popup').on('click', function(){
                        	$('.attributes').toggle();
                        })


                       /* $('#INTEREST').on('click', function(){
                          	var criteria = $('#INTEREST');
                          	criteria.appendTo('.attach-attributes');
