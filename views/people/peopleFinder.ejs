                           	$('#INTEREST_CLOSE').show();
                           	$('.attach-attributes').show();
                           	criteria.css('margin-bottom', 10);
                        });
						*/

						$('.attribute-item').on('click', function(){
                          	var criteria = $(this);

                          	if(criteria.hasClass('item-on-popup')) {
	                          	criteria.appendTo('.attach-attributes');
	                           	$('.attributes').hide();
	                           	$('.close-button').show();
	                           	$('.attach-attributes').show();
	                           	criteria.css('margin-bottom', 10);
	                           	criteria.removeClass('item-on-popup');
	                        }
	                        
                        });

					/*
                        $('#CHARACTERISTICS').on('click', function(){
